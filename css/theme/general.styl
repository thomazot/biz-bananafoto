
/**
 * Mixins
 */
icon(i)
    content: i
    text-transform: none
    font-weight: normal
    font-style: normal
    font-variant: normal
    font-family: 'biz_icons'
    speak: none
    -moz-osx-font-smoothing: grayscale
    -webkit-font-smoothing: antialiased

/**
 * Mixin - Font-weight
 */
font = 'Open Sans', sans-serif
light = 300
regular = 400
medium = 500
semibold = 600
bold = 700
/**
 * Mixin - Viewport
 */
viewport = 1170px
gap = 32px
/**
 * Mixin - Responsive
 * @media (max-width screen-sm-min)
 * @media (max-width screen-md-min)
 * @media (max-width screen-lg-min)
 */
screen-xxs = 320px
// Small devices (phones, 568px and down)
screen-xs = 568px
// Small devices (tablets, 768px and down)
screen-sm = 768px
// Medium devices (desktops, 992px and down)
screen-md = 992px
// Medium devices (desktops, 1200px and down)
screen-lg = 1200px

$container
    margin-right: auto
    margin-left: auto
    max-width: 'calc(100% - %s)' % gap
    width: viewport

boleto = 'data:image/png;base64,'
card = 'data:image/png;base64,'
pays = 'data:image/png;base64,'
security = 'data:image/png;base64,'

category-block()
    .categories
        .parent:after
            display: none

        &--justify
            width: 100%

            .ul--1
                display: block
                flex: 1
                overflow: visible
                min-width: 0
                max-width: none
                width: auto
                width: 100%
                column-count: 4

            .li--0
                position: static
                flex: 1 0 100px
                order: 1
                padding: 15px 4px
                min-width: 0

                &:hover
                    .a--0
                        background: #fff
                        box-shadow: 0 0 0 2px color2_bg, 0 5px 0 color2_bg
                        color: var(--black-light)

                        svg
                            color: color2_bg

                    .box--1
                        left: 50%
                        visibility: visible
                        opacity: 1
                        transform: translateX(-50%)

            .li--1
                break-inside: avoid

            .box--1
                top: 100%
                left: 50%
                z-index: 20
                visibility: hidden
                padding-top: 15px
                padding-bottom: 15px
                max-width: 100%
                width: 100%
                width: viewport
                box-shadow: none
                box-shadow: 800px 0 0 #fff, -800px 0 0 #fff
                opacity: 0
                transition: all .2s ease
                transform: translateX(-50%)

            .a--0
                display: flex
                flex-flow: column nowrap
                justify-content: center
                padding: 14px
                height: auto
                height: 100%
                border-radius: 4px
                background: #fff
                color: var(--black-light)
                white-space: wrap
                font-weight: regular
                font-size: 14px

                img
                    margin: 0
                    height: 50px
                    object-fit: contain

                svg
                    margin: 0
                    width: 53px
                    height: 36px
                    color: var(--dark)

                span
                    display: block
                    overflow: hidden
                    margin-top: 10px
                    max-width: 100%
                    height: auto
                    text-overflow: ellipsis
                    white-space: nowrap
                    line-height: 1.2

            .a--1
                color: color1_bg
                font-weight: regular
                font-size: 18px

                &:hover
                    box-shadow: none

            .a--2
                display: flex
                align-items: flex-start
                padding: 5px 0
                color: var(--black-light)
                font-weight: regular
                font-size: 16px

                &:before
                    flex: 0 0 10px
                    margin: 7px 10px 0 0
                    width: 10px
                    height: 10px
                    background: var(--menu-gray)
                    content: ''

                &:hover
                    box-shadow: none
                    color: color1_bg

                    &:before
                        background: color2_bg

        &__products, &__block
            position: relative
            flex: 0 1 230px
            padding-bottom: 80px
            border: solid 2px color2_bg
            border-left: 0
            border-radius: 8px

            .title--sm
                display: none

            .product
                &__main
                    position: static
                    margin-top: 15px
                    padding: 0
                    text-align: center

                &__name
                    margin-bottom: 5px
                    color: var(--black)
                    font-weight: regular
                    font-size: 13px

                    &:after
                        position: absolute
                        bottom: 20px
                        left: 50%
                        display: flex
                        justify-content: center
                        align-items: center
                        width: calc(100% - 40px)
                        height: 45px
                        border-radius: 45px
                        background: var(--green)
                        color: var(--white)
                        content: 'Comprar'
                        font-weight: bold
                        font-size: 14px
                        transform: translateX(-50%)

            .products
                &__item
                    padding-bottom: 0

                &__case
                    position: static

                    .price-box
                        margin-bottom: 0

                    &:hover
                        box-shadow: none

            .old-price, .price-box-avista, .price-box-parcelado, .product-rate
                display: none

        &__all
            .all-ul--2
                display: none

            .all-a--0
                font-weight: regular

                img, svg
                    display: none

            .a--0
                .z-more
                    width: 32px
                    height: 32px
                    color: color2_bg

                > span
                    margin-top: 0

                    span
                        margin-top: 8px
                        line-height: 1.2

            &.li--0
                order: 2

                &:hover
                    .a--0
                        svg
                            fill: color2_bg

title()
    margin: 30px 0
    border-bottom: solid 1px #dddddd
    color: var(--black)
    font-weight: regular
    font-size: 18px

    span
        position: relative
        display: inline-block
        margin-bottom: -3px
        padding: 20px 0
        box-shadow: inset 0 -5px 0 color2_bg
        font-weight: semibold
